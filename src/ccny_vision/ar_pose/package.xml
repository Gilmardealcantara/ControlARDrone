<package>
  <name>ar_pose</name>
  <version>0.0.1</version>
  <description>Augmented Reality Marker Pose Estimation using ARToolkit</description>
  <maintainer email="a@a.com">Ivan Dryanovski</maintainer>
  <maintainer email="a@a.com">William Morris</maintainer>
  <maintainer email="a@a.com">Gautier Dumonteil et al.</maintainer>

  <license>GPL</license>

  <url type="website">http://www.ros.org/wiki/ar_pose</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Ivan Dryanovski</author>
  <author>William Morris</author>
  <author>Gautier Dumonteil et al.</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>libartoolkit</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>resource_retriever</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>cv_bridge</build_depend>
  <build_depend>message_generation</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>libartoolkit</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>resource_retriever</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>cv_bridge</run_depend>
  <run_depend>message_runtime</run_depend>

